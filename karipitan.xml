<?xml version="1.0"?>
<Container version="2">
    <Name>KARIPITAN</Name>
    <Repository>sebastienvermeille/karipitan</Repository>
    <Registry>https://hub.docker.com/r/sebastienvermeille/karipitan</Registry>
    <Network>bridge</Network>
    <MyIP/>
    <Privileged>false</Privileged>
    <Support>todo</Support>
    <Project/>
    <Overview>Karipitan allow you to manage better your RIKA stove with exclusives features. All you need is a rika-firenet
        account.
    </Overview>
    <Category>Network:Management Tools:</Category>
    <WebUI>https://[IP]:[PORT:8080]</WebUI>
    <TemplateURL/>
    <Icon>https://raw.githubusercontent.com/sebastienvermeille/unraid-docker-templates/main/images/karipitan_icon.png</Icon>
    <ExtraParams/>
    <PostArgs/>
    <DateInstalled>1527397055</DateInstalled>
    <DonateText/>
    <DonateLink/>
    <DonateImg/>
    <MinVer/>
    <Description>Todo</Description>
    <Networking>
        <Mode>bridge</Mode>
        <Publish>
            <Port>
                <HostPort>8080</HostPort>
                <ContainerPort>80</ContainerPort>
                <Protocol>tcp</Protocol>
            </Port>
        </Publish>
    </Networking>
    <Data>
        <Volume>
            <HostDir>/mnt/user/appdata/karipitan</HostDir>
            <ContainerDir>/config</ContainerDir>
            <Mode>rw</Mode>
        </Volume>
    </Data>
    <Environment/>
    <Labels/>
    <Config Name="Host Port" Target="8080" Default="80" Mode="tcp" Description="Container Port: 80" Type="Port" Display="always"
            Required="true" Mask="false">80</Config>
    <Config Name="AppData Config Path" Target="/config" Default="/mnt/user/appdata/karipitan" Mode="rw"
            Description="Container Path: /config"
            Type="Path" Display="advanced-hide" Required="true" Mask="false">/mnt/user/appdata/karipitan</Config>
</Container>
